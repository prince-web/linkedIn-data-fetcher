{
    "manifest_version": 3,
    "name": "LinkedIn Profile Scraper",
    "version": "1.0",
    "description": "Scrapes LinkedIn profiles and posts data to an API",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.linkedin.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]

   
}